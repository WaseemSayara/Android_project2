<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        app:cardCornerRadius="20dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="8dp">

            <TextView
                android:id="@+id/popHouseTitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:text="View Tenant"
                android:textAlignment="center"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Medium"
                android:textColor="#000000"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rentPopNameLabel"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/popHouseTitle"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#478049"
                android:text="Tenant Name"
                android:textAlignment="center"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rentPopNameTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/popHouseTitle"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/rentPopNameLabel"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="asnas nimer"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/rentPopEmailLabel"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopNameLabel"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#478049"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="Email"
                android:textAlignment="center"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rentPopEmailTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopNameTextView"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/rentPopEmailLabel"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="amas@gmail.com"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/rentPopGenderLabel"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopEmailLabel"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#478049"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="Gender"
                android:textAlignment="center"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rentPopGenderTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopEmailLabel"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/rentPopGenderLabel"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="Male"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/rentPopAddressLabel"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopGenderLabel"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#478049"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="Address"
                android:textAlignment="center"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rentPopAddressTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopGenderTextView"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/rentPopAddressLabel"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="2005"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/rentPopNationalityLabel"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopAddressLabel"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#478049"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="Nationality"
                android:textAlignment="center"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rentPopNationalityTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopAddressTextView"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/rentPopNationalityLabel"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="2"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
                android:textColor="@android:color/black" />


            <TextView
                android:id="@+id/rentPopPhoneLabel"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopNationalityLabel"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#478049"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="Phone Number"
                android:textAlignment="center"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rentPopPhoneTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopNationalityTextView"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/rentPopPhoneLabel"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="Rented"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/rentPopOccupationLabel"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopPhoneLabel"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#478049"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="Occupation"
                android:textAlignment="center"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rentPopOccupationTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopPhoneTextView"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/rentPopOccupationLabel"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="yes"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/rentPopSalaryLabel"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopOccupationLabel"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#478049"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="Salary"
                android:textAlignment="center"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rentPopSalaryTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopOccupationTextView"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/rentPopSalaryLabel"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="12/3/3432"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
                android:textColor="@android:color/black" />

            <TextView
                android:id="@+id/rentPopFamilyLabel"
                android:layout_width="135dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopSalaryLabel"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:background="#478049"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="Family Members"
                android:textAlignment="center"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small.Inverse"
                android:textColor="@android:color/white"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/rentPopFamilyTextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/rentPopSalaryLabel"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_toRightOf="@id/rentPopFamilyLabel"
                android:paddingLeft="15dp"
                android:paddingRight="15dp"
                android:text="12/3/3432"
                android:textAppearance="@style/Base.TextAppearance.AppCompat.Small"
                android:textColor="@android:color/black" />


        </RelativeLayout>

    </android.support.v7.widget.CardView>


</LinearLayout>